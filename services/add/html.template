<h1>Default service</h1>
<div class="row" ng-controller="ontologyFormList">

	<div class="table-responsive">
		<table class="table table-hover">
			<thead>
				<tr>
					<th>S</th><th>P</th><th>O</th>
				</tr>
			</thead>
			<tbody>
				{%for row in queries.main%}
