    rdfs:isDefinedBy <http://xmlns.com/foaf/0.1/> ;
    rdfs:label "nickname" ;
    vs:term_status "testing" .

foaf:openid
    a rdf:Property, owl:InverseFunctionalProperty, owl:ObjectProperty ;
